# QuickKart Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# GOOGLE MAPS API CONFIGURATION
# =============================================================================
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Required for location detection and address autocomplete
# Make sure to enable: Maps JavaScript API, Places API, Geocoding API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# =============================================================================
# BLINKIT API CONFIGURATION (Optional)
# =============================================================================
# These are used for Blinkit API requests. Defaults work but may need updating.
BLINKIT_DEFAULT_LAT=28.4652382
BLINKIT_DEFAULT_LON=77.0615957
BLINKIT_WEB_APP_VERSION=1008010016
BLINKIT_APP_VERSION=1010101010

# =============================================================================
# FLASK CONFIGURATION
# =============================================================================
FLASK_ENV=development